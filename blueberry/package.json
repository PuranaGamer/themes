{
	"name": "blueberry",
	"version": "1.0.1",
	"description": "A Modern Dark theme for PHPMyAdmin",
	"author": "Shahab Movahhedi",
	"url": "https://shmovahhedi.com/",
	"supports": [
		"5.0",
		"5.1"
	],
	"themefolder": "F:\\xampp\\phpMyAdmin\\themes\\blueberry",
	"scripts": {
		"scss-compile-nomini": "sass scss:css",
